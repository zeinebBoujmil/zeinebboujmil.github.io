{
  "version": 2,
  "functions": {
    "index.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    {
      "src": "/(.*\\.css)",
      "headers": {
        "Content-Type": "text/css"
      },
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.xml)",
      "headers": {
        "Content-Type": "application/xml"
      },
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.xsl)",
      "headers": {
        "Content-Type": "application/xml"
      },
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/index.php"
    }
  ]
}
